    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.32.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "unplugin-vue-macros": "^1.7.3",
